{
  "name": "express-middleware-server",
  "version": "1.0.0",
  "scripts": {
    "build": "yarn run tsc",
    "postinstall": "yarn run build",
    "test": "yarn run nyc mocha test/test.ts"
  },
  "bin": {
    "server": "./bin/server"
  },
  "repository": "git@github.com:walk8243/express-middleware-server.git",
  "author": "walk8243 <walk8243@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "express": "^4.17.1",
    "typescript": "^3.5.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.0",
    "@types/mocha": "^5.2.7",
    "@types/node": "12.6.2",
    "@types/sinon": "^7.0.13",
    "@types/sinon-express-mock": "^1.3.7",
    "mocha": "^6.1.4",
    "nyc": "^14.1.1",
    "sinon": "^7.3.2",
    "sinon-express-mock": "^2.2.0",
    "ts-node": "^8.3.0"
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "*.d.ts"
    ],
    "require": [
      "ts-node/register"
    ]
  }
